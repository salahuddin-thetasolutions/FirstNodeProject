<html>
<head>
</head>
<body>
<%-  include partial_views/header.ejs %>
<div class="container-fluid">
<div id="demo" class="carousel slide" data-ride="carousel">

  <!-- Indicators -->
  <ul class="carousel-indicators">
    <li data-target="#demo" data-slide-to="0" class="active"></li>
    <li data-target="#demo" data-slide-to="1"></li>
    <li data-target="#demo" data-slide-to="2"></li>
  </ul>

  <!-- The slideshow -->
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="images/la.jpg" alt="Los Angeles">
    </div>
    <div class="carousel-item">
      <img src="images/chicago.jpg" alt="Chicago">
    </div>
    <div class="carousel-item">
      <img src="images/ny.jpg" alt="New York">
    </div>
  </div>

  <!-- Left and right controls -->
  <a class="carousel-control-prev" href="#demo" data-slide="prev">
    <span class="carousel-control-prev-icon"></span>
  </a>
  <a class="carousel-control-next" href="#demo" data-slide="next">
    <span class="carousel-control-next-icon"></span>
  </a>

</div>
<div class="row">
<div class="col-md-3">
<ul class="list-group">
  <li class="list-group-item d-flex justify-content-between align-items-center">
    <a class="nav-link active show" data-toggle="tab" href="#mobile">Mobiles</a>
  </li>
   <li class="list-group-item d-flex justify-content-between align-items-center">
    <a class="nav-link" data-toggle="tab" href="#shirts">Shirts</a>
  </li>
</ul>
</div>
<div class="col-md-9 tab-content">
//mobile
<div class="tab-pane fade active show" id="mobile">
<div class="row">
<div class="col-md-3">1</div>
<div class="col-md-3">2</div>
<div class="col-md-3">3</div>
</div>
</div>
//end mobile
//shirts
<div class="tab-pane fade" id="shirts">
<div class="row">
<div class="col-md-3">shirt1</div>
<div class="col-md-3">shirt2</div>
<div class="col-md-3">shirt3</div>
</div>
</div>
//end shirts
</div>

</div>
</div>

<h1> <%= Name+" "+RollNo %> </h1>

<h3> <%= RollNo %> </h3>
<div class="row">
	<div class="col-md-8">
		<%-  include partial_views/item_menu.ejs %>
	</div>
</div>

<%-  include partial_views/footer.ejs %>

</body>
</html>